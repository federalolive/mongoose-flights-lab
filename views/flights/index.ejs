<!DOCTYPE html>
<html lang="en">
  <%- include('../partials/head.ejs') %> 
<body>
  <%- include('../partials/nav.ejs') %>
  <h1>All Flights</h1>
  <table>
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight Number</th>
        <th>Depart</th>
      </tr>
    </thead>
    <tbody>
      <!-- Write the line of EJS to iterate over movies using forEach -->
      <% flights.forEach(function(f) { %>
        <tr>
          <td><%= f.airline %></td>
          <td><%= f.airport %></td>
          <td><%= f.flightNo %></td>
          <td><%= f.departs %></td>
          </tr>
      <% }) %> 
    </tbody>
  </table>
</body>
</html>